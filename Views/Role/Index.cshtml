@model IEnumerable<SZS.Models.CustomRole>
@{
    ViewBag.Title = "Role";
}
<div class="dodajSzkolenie">
    @Html.ActionLink("Dodaj nową", "Create")
    </div>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>

            </tr>
        }

    </table>

